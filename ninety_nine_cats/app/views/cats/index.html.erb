<h1>All Cats</h1>

<% @cats.each do |cat| %>
<ul>
    <li>Name: <%= cat.name %></li>
    <li>Birth date: <%= cat.birth_date %></li>
    <li>Color: <%= cat.color %></li>
    <li>Sex: <%= cat.sex %></li>
    <li>Age: <%= cat.age %></li>
    <li>Description: <%= cat.description %></li>
    <a href="<%= cat_url(cat) %>">View Cat</a>
</ul>
<% end %>